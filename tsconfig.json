{
    "$schema": "http://json.schemastore.org/tsconfig",
    "compilerOptions": {
        "strict": true,
        "strictFunctionTypes": false,
        "module": "commonjs",
        "target": "es2018",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "declaration": true,
        "lib": ["es2018"],
        "types": ["node"],
        "baseUrl": ".",
        "skipLibCheck": true
    }
}
